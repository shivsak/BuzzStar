CLANG_CXX_LANGUAGE_STANDARD = c++11
CLANG_CXX_LIBRARY = libc++
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/ApplePayStubs" "${PODS_ROOT}/Headers/Public/PaymentKit" "${PODS_ROOT}/Headers/Public/SSKeychain" "${PODS_ROOT}/Headers/Public/SVProgressHUD" "${PODS_ROOT}/Headers/Public/SVPullToRefresh" "${PODS_ROOT}/Headers/Public/Stripe" "${PODS_ROOT}/Headers/Public/pop"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/ApplePayStubs" -isystem "${PODS_ROOT}/Headers/Public/PaymentKit" -isystem "${PODS_ROOT}/Headers/Public/SSKeychain" -isystem "${PODS_ROOT}/Headers/Public/SVProgressHUD" -isystem "${PODS_ROOT}/Headers/Public/SVPullToRefresh" -isystem "${PODS_ROOT}/Headers/Public/Stripe" -isystem "${PODS_ROOT}/Headers/Public/pop"
OTHER_LDFLAGS = -ObjC -l"Pods-Buzzr-ApplePayStubs" -l"Pods-Buzzr-PaymentKit" -l"Pods-Buzzr-SSKeychain" -l"Pods-Buzzr-SVProgressHUD" -l"Pods-Buzzr-SVPullToRefresh" -l"Pods-Buzzr-Stripe" -l"Pods-Buzzr-pop" -l"c++" -framework "Foundation" -framework "QuartzCore" -framework "Security" -weak_framework "AddressBook" -weak_framework "PassKit"
OTHER_LIBTOOLFLAGS = $(OTHER_LDFLAGS)
PODS_ROOT = ${SRCROOT}/Pods